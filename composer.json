{
    "name": "innmind/infrastructure-amqp",
    "type": "library",
    "description": "Tool to install rabbitmq",
    "keywords": ["rabbitmq"],
    "homepage": "http://github.com/Innmind/InfrastructureAMQP",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/InfrastructureAMQP/issues"
    },
    "require": {
        "php": "~7.4",
        "innmind/cli": "~2.0",
        "innmind/operating-system": "~2.0",
        "innmind/immutable": "~3.5",
        "innmind/rabbitmq-management": "~2.0",
        "innmind/event-bus": "~4.0",
        "innmind/installation-monitor": "~3.0",
        "innmind/silent-cartographer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Infrastructure\\AMQP\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Infrastructure\\AMQP\\": "tests/",
            "Fixture\\Innmind\\Infrastructure\\AMQP\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "giorgiosironi/eris": "^0.11.0"
    },
    "bin": ["infrastructure-amqp"],
    "extra": {
        "gene": {
            "expression": [
                "infrastructure-amqp install",
                "infrastructure-amqp setup-users"
            ]
        }
    }
}
