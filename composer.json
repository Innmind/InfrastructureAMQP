{
    "name": "innmind/infrastructure-amqp",
    "type": "library",
    "description": "Tool to install rabbitmq",
    "keywords": ["rabbitmq"],
    "homepage": "http://github.com/Innmind/InfrastructureAMQP",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/InfrastructureAMQP/issues"
    },
    "require": {
        "php": "~7.4",
        "innmind/rabbitmq-management": "~2.0",
        "innmind/genome": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Infrastructure\\AMQP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Infrastructure\\AMQP\\": "tests/",
            "Fixture\\Innmind\\Infrastructure\\AMQP\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.4",
        "innmind/coding-standard": "^1.1"
    },
    "provide": {
        "innmind/genome-genes": "3.0"
    }
}
